{
  "name": "hono-openapi-prisma",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "start": "NODE_ENV=production node ./dist/src/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "tsc && eslint .",
    "lint:fix": "npm run lint --fix",
    "test": "cross-env NODE_ENV=test vitest",
    "build": "npm run db:generate-all && tsc && tsc-alias",
    "db:studio": "prisma studio",
    "db:pull": "prisma db pull --schema=./prisma/schema.prisma",
    "db:pull-x3": "prisma db pull --schema=./prisma/x3.prisma",
    "db:generate": "prisma generate",
    "db:generate-all": "npm run db:generate && prisma generate --schema=./prisma/x3.prisma"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.8",
    "@hono/zod-openapi": "^0.18.3",
    "@prisma/client": "^6.3.0",
    "@scalar/hono-api-reference": "^0.5.170",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "hono": "^4.6.20",
    "hono-pino": "^0.7.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "stoker": "1.4.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@types/node": "^22.13.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-plugin-format": "^1.0.1",
    "prisma": "^6.3.0",
    "prisma-dbml-generator": "^0.12.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5",
    "zod-prisma-types": "^3.2.1"
  }
}
